@model Agrokultura.Models.ContractPlot

@{
    ViewData["Title"] = "Create";
}

<div class="row row-udpate_create mx-auto">

    <div class="col-md-6 mx-auto col-update_create">
        <h1>Kreiraj novo!</h1>
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
       
            <div class="form-group form-group-update_create">
                <label asp-for="ContractId" class="control-label"></label>
                <select asp-for="ContractId" class ="form-control" asp-items="ViewBag.ContractId"></select>
            </div>
            <div class="form-group form-group-update_create">
                <label asp-for="PlotId" class="control-label"></label>
                <select asp-for="PlotId" class ="form-control" asp-items="ViewBag.PlotId"></select>
            </div>
            <div class="form-group form-group-update_create">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
        <div class="btn back-to-list">
            <a href="javascript:history.back()">Natrag</a>
        </div>
    </div>
</div>
